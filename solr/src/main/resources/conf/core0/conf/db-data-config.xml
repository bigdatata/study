<dataConfig>
    <dataSource driver="${db_driverClassName}" url="${db_url}" user="${db_username}" password="${db_password}"/>
    <document>
        <entity name="semKeywords" pk="id" batchSize="5000" preImportDeleteQuery="*:*"
                query="select id,
        platform,accountId,planId,adgroupId,keyword,price,quality,matchtype,pause,opstatus,valid,
        pcDestinationUrl,date_add(modifiedTime, interval +8 hour) as modifiedTime,status,
        date_add(createTime, interval +8 hour) as createTime from sem_keywords
        where '${dataimporter.request.clean}' != 'false' OR modifiedTime > '${dataimporter.last_index_time}'">
            <field name="id" column="id"/>
            <field name="platform" column="platform"/>
            <field name="accountId" column="accountId"/>
            <field name="planId" column="planId"/>
            <field name="groupId" column="adgroupId"/>
            <field name="price" column="price"/>
            <field name="quality" column="quality"/>
            <field name="matchType" column="matchType"/>
            <field name="pause" column="pause"/>
            <field name="opstatus" column="opstatus"/>
            <field name="valid" column="valid"/>
            <field name="modifiedTime" column="modifiedTime"/>
            <field name="createTime" column="createTime"/>
            <field name="status" column="status"/>

            <field name="keyword" column="keyword"/>
            <field name="keywordIndex" column="keyword"/>
            <field name="keywordPrefix" column="keyword"/>

            <field name="url" column="pcDestinationUrl"/>
            <field name="urlIndex" column="pcDestinationUrl"/>
            <field name="urlPrefix" column="pcDestinationUrl"/>

        </entity>
    </document>
</dataConfig>

